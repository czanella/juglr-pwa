{
  "name": "juglr-pwa",
  "version": "1.0.0",
  "description": "Juglr - PWA version",
  "author": "Carlos Zanella",
  "license": "MIT",
  "scripts": {
    "client:dev": "NODE_ENV=development node devServer/index.js",
    "client:build": "NODE_ENV=production webpack --config ./webpack.config.client.js",
    "server:build": "NODE_ENV=production webpack --config ./webpack.config.server.js",
    "html:build": "NODE_ENV=production webpack --config ./webpack.config.buildHtml.js",
    "serverless:build": "npm run client:build && npm run html:build",
    "build": "npm run server:build && npm run client:build",
    "start": "npm run client:dev",
    "test": "jest"
  },
  "dependencies": {
    "autoprefixer": "^7.2.6",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.10",
    "express": "^4.16.3",
    "file-loader": "^1.1.11",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.8.2",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "^2.2.0",
    "resolve-url-loader": "^2.3.0",
    "run-middleware": "^0.6.6",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^1.0.1",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.3",
    "webpack-merge": "^4.1.2",
    "webpack-node-externals": "^1.6.0"
  },
  "devDependencies": {
    "enzyme": "^2.9.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.7.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^20.0.4",
    "node-notifier": "^5.2.1",
    "opn": "^5.3.0",
    "react-hot-loader": "^4.3.1",
    "react-test-renderer": "^15.6.2",
    "sinon": "^2.4.1",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.2"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy"
    }
  }
}
